Algoritmo Valor_varibales
	//bloque declarativo//
	definir VAL1,VAL2,VAL3 como entero
	definir x,z como entero
	
	
	//Bloque de procesos//
	VAL1<- 4*15/5
	
	
	x= 3
	z= 2
	VAL2<- x^z-z
	
	
	VAL3<-5
	x=3
	
	
	VAL3<-VAL3*x
	
	//Bloque de salida//
	Mostrar "Valor de VAL1" VAL1
	Mostrar "Valor VAL2" VAL2
	Mostrar "Valor VAL3" VAL3
	

	
	
	
FinAlgoritmo
